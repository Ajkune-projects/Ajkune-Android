<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BLACK.DIALOG" parent="Theme.AjkuneProfessional">
        <!--<item name="android:windowFullscreen">true</item>-->
        <item name="android:statusBarColor">#00ffffff</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@color/cl_transparent_black</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>

    <style name="MY.DIALOG" parent="Theme.AjkuneProfessional">
        <!--<item name="android:windowFullscreen">true</item>-->
        <item name="android:statusBarColor">#00ffffff</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowBackground">@color/cl_transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>

    <style name="SelectableItemTheme">
        <item name="colorControlHighlight">#23a8466f</item>
    </style>

    <style name="SelectableItemBackground">
        <item name="android:theme">@style/SelectableItemTheme</item>
        <item name="android:background">?attr/selectableItemBackground</item>
    </style>

</resources>